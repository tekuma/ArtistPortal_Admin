package com.zhhb.system.entity;


import java.util.Date;
import com.pintu.pub.entity.BaseEntity;

@SuppressWarnings("serial")
public class User extends BaseEntity {
	
	private Integer bh;//编号
	private String dlmc;//登陆名称
	private String dlmm;//登陆密码
	private String yhxm;//用户姓名
	private Integer gnqx;//功能权限
	
	
	
	
	public Integer getBh() {
		return bh;
	}
	public void setBh(Integer bh) {
		this.bh = bh;
	}
	public String getDlmc() {
		return dlmc;
	}
	public void setDlmc(String dlmc) {
		this.dlmc = dlmc;
	}
	public String getDlmm() {
		return dlmm;
	}
	public void setDlmm(String dlmm) {
		this.dlmm = dlmm;
	}
	public String getYhxm() {
		return yhxm;
	}
	public void setYhxm(String yhxm) {
		this.yhxm = yhxm;
	}
	public Integer getGnqx() {
		return gnqx;
	}
	public void setGnqx(Integer gnqx) {
		this.gnqx = gnqx;
	}
	
	
	
	
}
