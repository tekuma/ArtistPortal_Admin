package com.zhhb.system.entity;

import java.util.Date;
import com.pintu.pub.entity.BaseEntity;

@SuppressWarnings("serial")
public class Memberconsume extends BaseEntity {
	
	private Integer bh;//编号
	private Integer hybh;//会员编号
	private Date xfsj;//消费时间
	private Float xfed;//消费额度
	private String xfxs;//消费形式：积分、货币
	private String xfgn;//消费功能
	private String xtbm;//系统表名
	private Integer zjz;//主键值
	public Integer getBh() {
		return bh;
	}
	public void setBh(Integer bh) {
		this.bh = bh;
	}
	public Integer getHybh() {
		return hybh;
	}
	public void setHybh(Integer hybh) {
		this.hybh = hybh;
	}
	public Date getXfsj() {
		return xfsj;
	}
	public void setXfsj(Date xfsj) {
		this.xfsj = xfsj;
	}
	public Float getXfed() {
		return xfed;
	}
	public void setXfed(Float xfed) {
		this.xfed = xfed;
	}
	public String getXfxs() {
		return xfxs;
	}
	public void setXfxs(String xfxs) {
		this.xfxs = xfxs;
	}
	public String getXfgn() {
		return xfgn;
	}
	public void setXfgn(String xfgn) {
		this.xfgn = xfgn;
	}
	public String getXtbm() {
		return xtbm;
	}
	public void setXtbm(String xtbm) {
		this.xtbm = xtbm;
	}
	public Integer getZjz() {
		return zjz;
	}
	public void setZjz(Integer zjz) {
		this.zjz = zjz;
	}
	
	
	
	
}
