package com.zhhb.system.entity;

import java.util.Date;
import com.pintu.pub.entity.BaseEntity;

@SuppressWarnings("serial")
public class Attention extends BaseEntity {
	
	private Integer bh;//编号
	private Integer zzbh;//作者编号
	private Integer hybh;//会员编号
	private Date gzsj;//关注时间
	private String hync;//会员昵称
	private Integer hdjf;//获得积分
	
	public Integer getBh() {
		return bh;
	}
	public void setBh(Integer bh) {
		this.bh = bh;
	}
	public Integer getZzbh() {
		return zzbh;
	}
	public void setZzbh(Integer zzbh) {
		this.zzbh = zzbh;
	}
	public Integer getHybh() {
		return hybh;
	}
	public void setHybh(Integer hybh) {
		this.hybh = hybh;
	}
	public Date getGzsj() {
		return gzsj;
	}
	public void setGzsj(Date gzsj) {
		this.gzsj = gzsj;
	}
	public String getHync() {
		return hync;
	}
	public void setHync(String hync) {
		this.hync = hync;
	}
	public Integer getHdjf() {
		return hdjf;
	}
	public void setHdjf(Integer hdjf) {
		this.hdjf = hdjf;
	}
	
	
	
	
}
