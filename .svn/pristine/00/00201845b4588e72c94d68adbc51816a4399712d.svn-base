package com.zhhb.system.entity;

import java.util.Date;
import com.pintu.pub.entity.BaseEntity;

@SuppressWarnings("serial")
public class Member extends BaseEntity {
	
	private Integer bh;//编号
	private String dlmc;//登录名称
	private String dlmm;//登陆密码
	private Date zcsj;//注册时间
	private String yzm;//验证码
	private String sjhm;//手机号码
	private String yx;//邮箱
	private Integer dqjf;//当前积分
	private Integer yyjg;//已用积分
	private String bz;//备注
	private String nc;//昵称
	public Integer getBh() {
		return bh;
	}
	public void setBh(Integer bh) {
		this.bh = bh;
	}
	public String getDlmc() {
		return dlmc;
	}
	public void setDlmc(String dlmc) {
		this.dlmc = dlmc;
	}
	public String getDlmm() {
		return dlmm;
	}
	public void setDlmm(String dlmm) {
		this.dlmm = dlmm;
	}
	public Date getZcsj() {
		return zcsj;
	}
	public void setZcsj(Date zcsj) {
		this.zcsj = zcsj;
	}
	public String getYzm() {
		return yzm;
	}
	public void setYzm(String yzm) {
		this.yzm = yzm;
	}
	public String getSjhm() {
		return sjhm;
	}
	public void setSjhm(String sjhm) {
		this.sjhm = sjhm;
	}
	public String getYx() {
		return yx;
	}
	public void setYx(String yx) {
		this.yx = yx;
	}
	public Integer getDqjf() {
		return dqjf;
	}
	public void setDqjf(Integer dqjf) {
		this.dqjf = dqjf;
	}
	public Integer getYyjg() {
		return yyjg;
	}
	public void setYyjg(Integer yyjg) {
		this.yyjg = yyjg;
	}
	public String getBz() {
		return bz;
	}
	public void setBz(String bz) {
		this.bz = bz;
	}
	public String getNc() {
		return nc;
	}
	public void setNc(String nc) {
		this.nc = nc;
	}
	
	
	
}
